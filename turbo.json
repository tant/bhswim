{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "apps/backend#build": {
      "outputs": ["apps/backend/.medusa/**/*"]
    },
    "apps/frontend#build": {
      "outputs": ["apps/frontend/dist/**"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}